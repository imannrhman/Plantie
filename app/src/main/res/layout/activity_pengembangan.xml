<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.PengembanganActivity">

    <com.github.florent37.awesomebar.AwesomeBar
        android:id="@+id/bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:bar_actionsColor="@color/colorPrimary"
        android:background="@android:color/white"
        app:bar_primaryColor="@android:color/white"
        app:bar_primaryDarkColor="@color/colorPrimary"
        android:elevation="4dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:id="@+id/ImagePengembangan"
        android:layout_width="219dp"
        android:layout_height="166dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/ic_undraw_version_control_" />

    <TextView
        android:id="@+id/TitlePengembangan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fitur sedang dalam
                      Pengembangan :)"
        android:textSize="16sp"
        android:fontFamily="@font/montserrat"
        android:layout_marginTop="20dp"
        android:textAlignment="center"
        app:layout_constraintTop_toBottomOf="@id/ImagePengembangan"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <View
        android:id="@+id/daunBottom"
        android:layout_width="248dp"
        android:layout_height="207dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/vector_daun_bottom"/>


</androidx.constraintlayout.widget.ConstraintLayout>